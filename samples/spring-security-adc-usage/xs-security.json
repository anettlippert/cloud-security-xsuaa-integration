{
	"xsappname": "adc-user",
	"tenant-mode": "shared",
	"scopes": [
		{
			"name": "$XSAPPNAME.Read",
			"description": "Read Permissions."
		},
		{
			"name": "$XSAPPNAME.Admin",
			"description": "Admin permissions."
		},
		{
			"name": "$XSAPPNAME.Write",
			"description": "Write permissions."
		}
	],
	"role-templates": [
		{
			"name": "Viewer",
			"description": "View Data",
			"scope-references": [
				"$XSAPPNAME.Read"
			]
		},
		{
			"name": "Administrator",
			"description": "View Sensitive Data",
			"scope-references": [
				"$XSAPPNAME.Read",
				"$XSAPPNAME.Admin"
			]
		},
		{
			"name": "Changer",
			"description": "Change data",
			"scope-references": [
				"$XSAPPNAME.Read",
				"$XSAPPNAME.Write"
			]
		}
	],
	"role-collections": [
		{
			"name": "Viewer",
			"description": "Viewer (read)",
			"role-template-references": [
				"$XSAPPNAME.Viewer"
			]
		},
		{
			"name": "Administrator",
			"description": "Administrator (read all)",
			"role-template-references": [
				"$XSAPPNAME.Administrator"
			]
		},
		{
			"name": "Viewer2",
			"description": "Viewer (read)",
			"role-template-references": [
				"$XSAPPNAME.Viewer"
			]
		},
		{
			"name": "Changer",
			"description": "Changer",
			"role-template-references": [
				"$XSAPPNAME.Changer"
			]
		}
	]
}  
